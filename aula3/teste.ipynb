    "def indice_menor(lista):\n",
    "    num = lista[0]\n",
    "    for i in lista:\n",
    "        if i < num:\n",
    "            num = i\n",
    "    for j in lista:\n",
    "        if lista[j] == num:\n",
    "            return j"
